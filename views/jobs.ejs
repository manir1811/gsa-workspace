<% include ./partials/header %>

<h4 class="center">Completed Jobs<a href="/jobs/new" class="waves-effect waves-light btn right">Add</a></h4>

    <ul class="collapsible" data-collapsible="accordion">
        <% jobs.forEach(function(job){ %>
        <li>
          <div class="collapsible-header">
                  <div class="row" style="width:100%">
                      <div class="col s3">
                        Job #: <%= job.job_num %>
                      </div>
                      <div class="col s3">
                        IMO: <%= job.ship.name %>
                      </div>
                      <div class="col s3">
                       Agency: <%= job.agency %>
                      </div>
                      <div class="col s2">
                        Port: <%= job.terminal %>
                      </div>
                      <div class="col s1">
                        <a href="/jobs/<%= job._id %>"><i class="material-icons">info_outline</i></a>
                      </div>
              </div>
          </div>
          <div class="collapsible-body grey lighten-4"><span>Lorem ipsum dolor sit amet.</span></div>
        </li>
       <% }); %>
      </ul>

<% include ./partials/footer %>